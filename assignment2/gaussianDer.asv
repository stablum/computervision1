function [ imOut Gd ] = gaussianDer(image_path, G, sigma)
	G1d = gaussianDerVec(G, sigma);
	%gaussDerVec 
	im = imread(image_path);
	for i = 1:3
		im_curr_color = im(:,:,i);
		im_curr_color_convolved = convolution(im_curr_color, gauss);
		im_curr_color_convolved = convolution(im_curr_color_convolved, gauss');
		im(:,:,i) = im_curr_color_convolved;
	end;
	imOut = im;
end
